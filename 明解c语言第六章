  return 0;
}
6-8
#include <stdio.h>
int m(int v[n])
{
  int v[n],min,i;
  min=v[0];
  for(i=0;i<n;i++){
    if(v[i]<=min)
      min=v[i];
  }
  return min;
}
int main(void)
{
  int v[n];
  printf("数组中最小值是:%d",min);
  return 0;
}
6-9
#include <stdio.h>
int m(int v[n])
{
  int i=0,a;
  for(i=0;i<=n/2;i++)
    a=v[i];
    v[i]=v[n-1-i];
    v[n-1-i]=a;
  }
  return v[];
}
int main(void)
{
  int v[n];
  printf("数组的倒序排列
