6-1
#include <stdio.h>
int min2(int x,int y)
{
  (x>y) ? y : x
}
int main(void)
{
  int a,b;
  printf("请输入两个整数");
  printf("整数1:");scanf("%d",&a);
  printf("整数2:");scanf("%d",&b);
  printf("最小值是%d",min2(a,b));
  return 0;
}
6-2
#include <stdio.h>
int min2(int x,int y)
{
  (x>y) ? y : x
}
int main(void)
{
  int a,b,c;
  printf("请输入三个整数");
  printf("整数1:");scanf("%d",&a);
  printf("整数2:");scanf("%d",&b);
  printf("整数3:");scanf("%d",&c);
  printf("最小值是%d",min2(min2(a,b),c));
  return 0;
}
6-3
#include <stdio.h>
int lifang(int x)
{
  x=x*x*x;
  return x;
}
int main(void)
{
  int x;
  printf("x的立方是%d",lifang(x));
  return 0;
}
6-4
#include <stdio.h>
int lifang(int x)
{
  x=x*x*x*x;
  return x;
}
int main(void)
{
  int x;
  printf("x的四次方是%d",lifang(x));
  return 0;
}
6-5
#include <stdio.h>
int sum(int x)
{
  int i=1,y=0;
  for(i=1;i<=x;i++)
    y+=i;
  return y;
}
int main(void)
{
  int n;
  printf("1到n之间整数的和为%d",sum(n));
  return 0;
}
6-6
#include <stdio.h>
void alert(int n)
{
  int i=0;
  for(i=0;i<n;i++)
    putchar('\a');
}
int main(void)
{
  int n;
  printf("请输入一个整数:"):scanf("%d",&n);
  void alert(n);
  return 0;
}
